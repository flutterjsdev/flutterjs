    "keywords": [
        "flutter",
        "analyzer",
        "dart",
        "widget",
        "parser",
        "javascript"
    ],
    "dependencies": {
        "@flutterjs/runtime": "file:../../flutterjs_runtime/flutterjs_runtime",
        "@flutterjs/vdom": "file:../../flutterjs_vdom/flutterjs_vdom",
        "@flutterjs/material": "file:../../flutterjs_material/flutterjs_material"
    },
    "author": "jayprakash.pal@flutterjs.dev",
    "license": "MIT",
    "devDependencies": {
        "esbuild": "^0.27.2",
        "jest": "^29.7.0"
    }
}