{
  "comments": {
    "lineComment": "//",
    "blockComment": [
      "/*",
      "*/"
    ]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["<", ">"]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "<",
      "close": ">"
    },
    {
      "open": "`",
      "close": "`"
    },
    {
      "open": "\"",
      "close": "\""
    },
    {
      "open": "'",
      "close": "'"
    }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["<", ">"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^((?!///).*\\{[^}\"'`]*|.*\\([^)\"'`]*)$",
    "decreaseIndentPattern": "^(.*\\}[^{\"'`]*|.*\\)[^(\"'`]*);?$"
  },
  "folding": {
    "markers": {
      "start": "^\\s*//\\s*#region",
      "end": "^\\s*//\\s*#endregion"
    }
  }
}