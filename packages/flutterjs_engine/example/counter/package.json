{
  "name": "counter-app",
  "version": "1.0.0",
  "description": "A Flutter.js Counter Application - Demonstrating State Management & Material Design",
  "type": "module",
  "main": "dist/index.html",
  "module": "src/main.fjs",
  "scripts": {
    "dev": "flutterjs dev",
    "dev:debug": "flutterjs dev --debug",
    "dev:verbose": "flutterjs dev --verbose",
    "build": "flutterjs build",
    "build:prod": "flutterjs build --production",
    "build:analyze": "flutterjs build --analyze",
    "preview": "flutterjs preview",
    "analyze": "flutterjs analyze",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "clean": "rimraf dist .cache",
    "type-check": "flutterjs check --types"
  },
  "keywords": [
    "flutter",
    "flutter.js",
    "framework",
    "reactive",
    "ui",
    "web",
    "material-design"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@flutterjs/core": "^1.0.0",
    "@flutterjs/material": "file:../../../package/material",
    "@flutterjs/icons": "^1.0.0"
  },
  "devDependencies": {
    "@flutter-js/cli": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0"
  },
  "flutterjs": {
    "version": "1.0.0",
    "configFile": "flutterjs.config.js",
    "entry": {
      "main": "src/main.fjs",
      "rootWidget": "MyApp",
      "entryFunction": "main"
    },
    "build": {
      "source": "src",
      "output": "dist",
      "target": "web"
    },
    "dev": {
      "port": 3000,
      "hmr": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not IE 11"
  ]
}