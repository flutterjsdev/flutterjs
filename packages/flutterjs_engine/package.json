{
  "name": "flutterjs",
  "version": "1.0.0",
  "type": "module" ,
  "description": "Lightweight Flutter.js CLI with zero external dependencies - Build tool for Flutter to JavaScript projects",
  "main": "bin/index.js",
  "bin": {
    "flutterjs": "./bin/index.js"
  },
  "scripts": {
    "dev": "node bin/index.js dev",
    "build": "node bin/index.js build",
    "preview": "node bin/index.js preview",
    "run": "node bin/index.js run",
    "test": "node test/test.js",
    "build:binary": "pkg . --compress Brotli",
    "build:linux": "pkg . -t node18-linux-x64 -o dist/flutter-js-linux",
    "build:macos": "pkg . -t node18-macos-x64 -o dist/flutter-js-macos",
    "build:windows": "pkg . -t node18-win-x64 -o dist/flutter-js.exe",
    "build:all": "npm run build:linux && npm run build:macos && npm run build:windows"
  },
  "dependencies": {
    "jest": "^29.7.0",
    "chalk": "^5.6.2",
    "ora":"^9.0.0",
    "open": "^11.0.0",
    "http-proxy-middleware": "^3.0.5",
    "chokidar": "^5.0.0",
    "express": "^5.2.1",
    "ws": "^8.18.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "commander":"^14.0.2"
  },
  "keywords": [
    "flutter",
    "flutter.js",
    "flutter-js",
    "dart",
    "framework",
    "cli",
    "build-tool",
    "bundler",
    "ssr",
    "csr",
    "transpiler",
    "zero-dependency"
  ],
  "author": "jayprakash.pal@flutterjs.dev",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin/",
    "commands/",
    "src/",
    "src/analyzer/",
    "src/code_gen/",
    "src/seo/",
    "src/ssr/",
    "src/vdom/",
    "templates/",
    "utils/",
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "dist",
    "assets": [
      "templates/**/*",
      "dist/**/*"
    ]
  },
  
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flutterjsdev/flutterjs.git"
  },
  "bugs": {
    "url": "https://github.com/flutterjsdev/flutterjs/issues"
  },
   
  "homepage": "https://github.com/flutterjsdev/flutterjs#readme"
}